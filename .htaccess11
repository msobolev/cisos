# turn on the module for this directory
ExpiresActive on
# set default
ExpiresDefault "access plus 24 hours"
ExpiresByType image/jpg "access plus 1 months"
ExpiresByType image/gif "access plus 1 months"
ExpiresByType image/jpeg "access plus 1 months"
ExpiresByType image/png "access plus 1 months"
ExpiresByType css/images/jpg "access plus 1 months"
ExpiresByType css/images/gif "access plus 1 months"
ExpiresByType css/images/jpeg "access plus 1 months"
ExpiresByType css/images/png "access plus 1 months"

ExpiresByType images/spinner.gif "access plus 1 months"
ExpiresByType images/check-small.png "access plus 1 months"
ExpiresByType images/scroll.png "access plus 1 months"
ExpiresByType images/sharethis_16.png "access plus 1 months"
ExpiresByType images/sprite_32_small.png "access plus 1 months"

ExpiresByType text/css "access plus 1 months"
ExpiresByType text/javascript "access plus 1 months"
ExpiresByType application/javascript "access plus 1 months"
ExpiresByType application/x-shockwave-flash "access plus 1 months"

RewriteEngine On

Rewritecond %{http_host} ^ctosonthemove.com [nc]
RewriteRule ^(.*)$ https://www.ctosonthemove.com/$1 [r=301,nc]

#Rewritecond %{http_host} !^www\.ctosonthemove\.com [NC]
Rewritecond %{http_host} !^itleaders\.ctosonthemove\.com [NC]
Rewritecond %{http_host} !^itexecs\.ctosonthemove\.com [NC]
Rewritecond %{http_host} !^techleaders\.ctosonthemove\.com [NC]
